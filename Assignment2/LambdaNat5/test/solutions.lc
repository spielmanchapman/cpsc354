let rec member = \ elem. \list.
    if list = # then 0 else
        if (hd list) = elem then 1 else
            member elem (tl list)
in
member 1 2:3:1:#

;;

let rec remove = \elem. \list.
    if member (elem) (list) = 0 then 
        list
    else
        if (hd list) = elem 
            then (tl list)
        else 
            (hd list) : (remove (elem) (tl list))

in
remove 2 1:2:3:1:2:3:#